  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.2.4",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "pg": "^7.18.2",
