    "test:unit": "jest --color --detectOpenHandles --force-exit test/unit/",
    "test:integration": "jest --color --detectOpenHandles --force-exit test/integration/",
    "test:e2e": "jest --color --detectOpenHandles --force-exit test/e2e/",
